    <None Remove="Images\icon_lock.png" />
    <None Remove="Images\icon_user1.png" />
    <None Remove="Images\image_login.jpg" />
    <None Remove="Images\lock.png" />
    <None Remove="Images\phone.png" />
    <None Remove="Images\user.png" />
    <None Remove="Images\username.png" />
    <None Remove="Images\user_login.png" />
    <None Remove="Images\view.png" />
  </ItemGroup>

  <ItemGroup>
    <Content Include="Images\apellido.png">
      <CopyToOutputDirectory>PreserveNewest</CopyToOutputDirectory>
    </Content>
    <Content Include="Images\calendar.png">
      <CopyToOutputDirectory>PreserveNewest</CopyToOutputDirectory>
