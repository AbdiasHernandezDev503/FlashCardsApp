<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <OutputType>WinExe</OutputType>
    <TargetFramework>net6.0-windows</TargetFramework>
    <Nullable>enable</Nullable>
    <UseWindowsForms>true</UseWindowsForms>
    <ImplicitUsings>enable</ImplicitUsings>
  </PropertyGroup>

  <ItemGroup>
    <None Remove="Fonts\Raleway-Regular.ttf" />
    <None Remove="Fonts\Raleway-SemiBold.ttf" />
    <None Remove="Images\apellido.png" />
    <None Remove="Images\calendar.png" />
    <None Remove="Images\card.png" />
    <None Remove="Images\confirm.png" />
    <None Remove="Images\email.png" />
    <None Remove="Images\exit.png" />
